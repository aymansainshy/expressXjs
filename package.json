{
  "name": "@expressx/core",
  "version": "1.0.0",
  "description": "A lightweight, object-oriented, decorator-based framework on top of Express.js",
  "type": "commonjs",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "ts-node src/examples/index.ts"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/constants.js",
      "require": "./dist/constants/constants.js",
      "types": "./dist/constants/constants.d.ts"
    },
    "./base": {
      "import": "./dist/base/index.js",
      "require": "./dist/base/index.js",
      "types": "./dist/base/index.d.ts"
    },
    "./decorators": {
      "import": "./dist/decorators/index.js",
      "require": "./dist/decorators/index.js",
      "types": "./dist/decorators/index.d.ts"
    },
    "./http": {
      "import": "./dist/http/index.js",
      "require": "./dist/http/index.js",
      "types": "./dist/http/index.d.ts"
    },
    "./common": {
      "import": "./dist/common/index.js",
      "require": "./dist/common/index.js",
      "types": "./dist/common/index.d.ts"
    },
    "./framework": {
      "import": "./dist/framework/index.js",
      "require": "./dist/framework/index.js",
      "types": "./dist/framework/index.d.ts"
    },
    "./runtime": {
      "import": "./dist/runtime/index.js",
      "require": "./dist/runtime/index.js",
      "types": "./dist/runtime/index.d.ts"
    },
    "./scanner": {
      "import": "./dist/scanner/index.js",
      "require": "./dist/scanner/index.js",
      "types": "./dist/scanner/index.d.ts"
    },
    "./dicontainer": {
      "import": "./dist/dicontainer/index.js",
      "require": "./dist/dicontainer/index.js",
      "types": "./dist/dicontainer/index.d.ts"
    }
  },
  "dependencies": {
    "express": "5.2.1",
    "glob": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "tsconfig-paths": "^4.2.0",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "author": "Ayman",
  "license": "ISC"
}